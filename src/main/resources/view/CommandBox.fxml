<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.geometry.Pos?>

<StackPane styleClass="stack-pane" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER">
  <TextField fx:id="commandTextField" onAction="#handleCommandEntered" promptText="Enter command here..."/>
  <TextField fx:id="promptTextField" onAction="#handleCommandEntered" promptText="" disable="true" style="visibility: hidden; -fx-font-style: italic;"/>
</StackPane>
